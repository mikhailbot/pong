<h2>Show Host</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @host.name %>
  </li>

  <li>
    <strong>Ip address:</strong>
    <%= @host.ip_address %>
  </li>

</ul>

<span><%= link "Edit", to: host_path(@conn, :edit, @host) %></span>
<span><%= link "Back", to: host_path(@conn, :index) %></span>
